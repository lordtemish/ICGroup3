<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/light_grey"
    tools:context=".ObjectFragments.PassportObjectInfoListFragment">
    <android.support.v4.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="10sp"
        >
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/white_page"
            android:orientation="vertical"
            >
            <android.support.constraint.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="50sp">
                <TextView
                    android:id="@+id/title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:layout_marginStart="20sp"
                    android:layout_marginLeft="20sp"
                    android:textColor="@color/black"
                    android:text="Общие данные"
                    android:textSize="13sp"
                    />
                    <ImageView
                        android:id="@+id/arrowTop"
                        android:layout_width="30sp"
                        android:layout_height="30sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginEnd="20sp"
                        android:layout_marginRight="20sp"
                        android:padding="7dp"
                        android:src="@drawable/ic_arrowdown"
                        android:background="@drawable/green_circle"
                        />
            </android.support.constraint.ConstraintLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:visibility="visible"
                android:id="@+id/emplLa"
                android:layout_marginBottom="20dp"
                >
            <LinearLayout
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:layout_width="match_parent"
                android:layout_height="80sp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:id="@+id/infoListObjectNameTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Наименование объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                    <TextView
                        android:id="@+id/infoListRegionTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/infoListWorkStartTextView"
                        app:layout_constraintBottom_toTopOf="@+id/infoListObjectAreaTextView"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Регион"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                    <TextView
                        android:id="@+id/infoListObjectAddressTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:id="@+id/infoListObjectName"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                    <TextView
                        android:id="@+id/infoListRegion"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"

                        />
                    <TextView
                        android:id="@+id/infoListObjectAddress"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:layout_width="match_parent"
                android:layout_height="80sp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    >
                <TextView
                    android:id="@+id/infoListWorkStartTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    android:layout_marginStart="20sp"
                    android:layout_marginLeft="20sp"
                    android:textSize="11sp"
                    android:textColor="@color/darkgrey"
                    android:text="Начало работы"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/infoListWorkStopTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/infoListWorkStartTextView"
                    app:layout_constraintBottom_toTopOf="@+id/infoListObjectAreaTextView"
                    android:layout_marginStart="20sp"
                    android:layout_marginLeft="20sp"
                    android:textSize="11sp"
                    android:textColor="@color/darkgrey"
                    android:text="Конец работы"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/infoListObjectAreaTextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintBottom_toBottomOf="parent"
                    android:layout_marginStart="20sp"
                    android:layout_marginLeft="20sp"
                    android:textSize="11sp"
                    android:textColor="@color/darkgrey"
                    android:text="Площадь объекта"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    >
                <TextView
                    android:id="@+id/infoListWorkStart"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="11sp"
                    android:textColor="@color/black"
                    android:text="Адрес объекта"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    />
                <TextView
                    android:id="@+id/infoListWorkStop"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="11sp"
                    android:textColor="@color/black"
                    android:text="Адрес объекта"
                    android:layout_weight="1"
                    android:gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/infoListObjectArea"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"

                    android:textSize="11sp"
                    android:textColor="@color/black"
                    android:text="Адрес объекта"
                    android:layout_weight="1"
                    android:gravity="center_vertical"
                    />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="30sp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:id="@+id/infoListHeadTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Начальник производства"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:id="@+id/infoListHead"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text=""
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />

                </LinearLayout>
            </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/infoListAdvisorTextView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:paddingLeft="20dp"
                            android:paddingStart="20dp"
                            android:textSize="11sp"
                            android:textColor="@color/darkgrey"
                            android:text="Кураторы"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"
                            />

                        <TextView

                            android:id="@+id/infoListAdvisor"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:textSize="11sp"
                            android:textColor="@color/black"
                            android:text=""
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            android:layout_gravity="center"

                            />


                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <TextView
                            android:id="@+id/infoListAdministratorTextView"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="20sp"
                            android:layout_marginLeft="20sp"
                            android:textSize="11sp"
                            android:textColor="@color/darkgrey"
                            android:text="Администратор"
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            />
                        <TextView
                            android:id="@+id/infoListAdministrator"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:textSize="11sp"
                            android:textColor="@color/black"
                            android:text=""
                            android:layout_weight="1"
                            android:gravity="center_vertical"
                            />


                    </LinearLayout>
                </LinearLayout>
            <LinearLayout
                android:paddingTop="10sp"
                android:paddingBottom="10sp"
                android:layout_width="match_parent"
                android:layout_height="60sp"
                android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical"

                    >
                    <TextView
                        android:id="@+id/infoListClientTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Клиент"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />

                    <TextView
                        android:id="@+id/infoListClientPreTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginStart="20sp"
                        android:layout_marginLeft="20sp"
                        android:textSize="11sp"
                        android:textColor="@color/darkgrey"
                        android:text="Ответсвенное лицо"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical"
                    android:layout_weight="1"
                    >
                    <TextView
                        android:id="@+id/infoListClient"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />

                    <TextView
                        android:id="@+id/infoListClientPre"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:textSize="11sp"
                        android:textColor="@color/black"
                        android:text="Адрес объекта"
                        android:layout_weight="1"
                        android:gravity="center_vertical"
                        />
                </LinearLayout>
            </LinearLayout>
            </LinearLayout>
        </LinearLayout>
        <android.support.v7.widget.RecyclerView
            android:layout_marginTop="20sp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:nestedScrollingEnabled="false"
            android:id="@+id/phonesRecyclerView"
            >
        </android.support.v7.widget.RecyclerView>
    </LinearLayout>
    </android.support.v4.widget.NestedScrollView>
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone"
        android:background="@color/icgGreenTra"
        android:onClick="onClick"
        android:id="@+id/progressLayout"
        >
            <ProgressBar
                android:layout_width="50dp"
                android:layout_height="50dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                android:id="@+id/progressBar"
                />
    </android.support.constraint.ConstraintLayout>
</FrameLayout>